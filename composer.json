{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.8.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/refs/tags/1.8.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/dinbror/blazy/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "dinbror/blazy": "^1.8",
        "drupal/admin_toolbar": "^3.0",
        "drupal/back_to_top": "^2.0",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/bootstrap5": "^1.1",
        "drupal/bootstrap_horizontal_tabs": "^1.4",
        "drupal/config_actions": "^1.3",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/current_page_crumb": "^1.3",
        "drupal/devel": "^4.1",
        "drupal/ds": "^3.13",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/flag": "^4.0@beta",
        "drupal/flexible_weather_api": "^2.0",
        "drupal/fontawesome": "^2.19",
        "drupal/menu_item_extras": "^2.16",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/redirect": "^1.7",
        "drupal/slick": "^2.4",
        "drupal/slick_views": "^2.6",
        "drupal/smart_date": "^3.4",
        "drupal/social_media_links": "^2.8",
        "drupal/superfish": "^1.4",
        "drupal/time_field": "^2.0",
        "drupal/token": "^1.10",
        "drupal/twig_tools": "^1.4",
        "drupal/twig_tweak": "^3.1",
        "drupal/twig_vardumper": "^3.0",
        "drupal/vardumper": "^1.6",
        "drupal/video_embed_field": "^2.4",
        "drupal/viewfield": "^3.0@beta",
        "drupal/views_bootstrap": "^4.3",
        "drupal/views_fieldsets": "^3.4",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/webform": "^6.1",
        "drush/drush": "^11.0",
        "kenwheeler/slick": "^1.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
